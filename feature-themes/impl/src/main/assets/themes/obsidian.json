{
  "name": "Obsidian",
  "author": "Squircle CE",
  "type": "dark",
  "colors": {
    "editor.background": "#2A3134",
    "editor.foreground": "#E0E2E4",
    "editor.selectionBackground": "#616161",
    "editor.selectionHighlightBackground": "#838177",
    "highlightedDelimitersBackground": "#616161",
    "highlightedDelimitersForeground": "#E0E2E4",
    "completionWindowCorner": "#67777B",
    "completionWindowBackground": "#31393C",
    "completionWindowBackgroundCurrent": "#394144",
    "editor.lineHighlightBackground": "#31393C",
    "editorLineNumber.foreground": "#859599",
    "editorLineNumber.activeForeground": "#E0E0E0",
    "editorLineNumber.divider": "#67777B",
    "editorIndentGuide.background": "#67777B",
    "editorIndentGuide.activeBackground": "#E0E0E0",
    "editorCursor.foreground": "#BBBBBB",
    "editorHandle.foreground": "#9EC56F",
    "editorWhitespace.foreground": "#616366"
  },
  "tokenColors": [
    {
      "settings": {
        "background": "#2A3134",
        "foreground": "#E0E2E4"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#808C92"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#E7E2BC"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#DE7C2E"
      }
    },
    {
      "name": "String Interpolation",
      "scope": "punctuation.definition.template-expression",
      "settings": {
        "foreground": "#9EC56F"
      }
    },
    {
      "name": "String Interpolation Body",
      "scope": "expression.string,meta.template.expression",
      "settings": {
        "foreground": "#E0E2E4"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#F8CE4E"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language,variable.language,constant.character.escape",
      "settings": {
        "foreground": "#9EC56F"
      }
    },
    {
      "name": "User-defined constant",
      "scope": "constant.character,constant.other",
      "settings": {
        "foreground": "#F8CE4E"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword,keyword.operator.new,keyword.operator.delete,keyword.operator.expression",
      "settings": {
        "foreground": "#9EC56F"
      }
    },
    {
      "name": "Types, Class Types",
      "scope": "storage.type.generic,meta.return.type,meta.type.annotation,meta.type.parameters,support.type.primitive",
      "settings": {
        "foreground": "#F8CE4E"
      }
    },
    {
      "name": "Storage Type",
      "scope": "storage,storage.type,storage.modifier",
      "settings": {
        "foreground": "#9EC56F"
      }
    },
    {
      "name": "Class constructor",
      "scope": "new.expr entity.name.type",
      "settings": {
        "foreground": "#E7E2BC"
      }
    },
    {
      "name": "Function",
      "scope": "support.function,entity.name.function",
      "settings": {
        "foreground": "#E7E2BC"
      }
    },
    {
      "name": "Function Argument",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#E0E2E4"
      }
    },
    {
      "name": "Variable, Property",
      "scope": "variable.property,variable.other.property,variable.other.object.property,variable.object.property,support.variable.property,meta.object-literal.key",
      "settings": {
        "foreground": "#6E8BAE"
      }
    },
    {
      "name": "Markup Headings",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#E7E2BC"
      }
    },
    {
      "name": "Markup Links",
      "scope": "markup.underline.link",
      "settings": {
        "fontStyle": "underline",
        "foreground": "#DE7C2E"
      }
    },
    {
      "name": "Tag name",
      "scope": "punctuation.definition.tag,entity.name.tag",
      "settings": {
        "foreground": "#9EC56F"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#E0E2E4"
      }
    },
    {
      "name": "Tag value",
      "scope": "support.constant.property-value,string.quoted.single.tag,string.quoted.double.tag,string.unquoted.tag",
      "settings": {
        "foreground": "#DE7C2E"
      }
    },
    {
      "name": "Documentation Block",
      "scope": "comment.block.documentation",
      "settings": {
        "foreground": "#808C92"
      }
    },
    {
      "name": "Documentation Highlight",
      "scope": "keyword.other.documentation",
      "settings": {
        "foreground": "#808C92"
      }
    },
    {
      "name": "Documentation Entity",
      "scope": "entity.name.type.instance.jsdoc",
      "settings": {
        "foreground": "#808C92"
      }
    },
    {
      "name": "Regex",
      "scope": "string.regexp,punctuation.definition.group.regexp,punctuation.definition.character-class.regexp",
      "settings": {
        "foreground": "#9EC56F"
      }
    },
    {
      "name": "Annotation, Preprocessor",
      "scope": "meta.preprocessor,entity.name.other.preprocessor,storage.type.annotation,punctuation.definition.annotation",
      "settings": {
        "foreground": "#9B84B9"
      }
    },
    {
      "name": "CSS ID, Selector",
      "scope": "meta.selector.css,entity.other.attribute-name.class,entity.other.attribute-name.pseudo-class.css,entity.other.attribute-name.pseudo-element.css",
      "settings": {
        "foreground": "#E7E2BC"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796E6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#CD9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#F44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#B267E6"
      }
    },
    {
      "name": "Ignored (Exceptions Rules)",
      "scope": "punctuation.definition.typeparameters.begin,punctuation.definition.typeparameters.end",
      "settings": {
        "foreground": "#E0E2E4"
      }
    }
  ],
  "semanticHighlighting": true
}
